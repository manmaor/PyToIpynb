{
	"name": "pytoipynb",
	"displayName": "PyToIpynb",
    "description": "save current python file as ipynb",
    "icon": "./logo/logo.png",
    "version": "1.0.0",
    "publisher": "maorbarak",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [ 
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
        "menus": {
            "commandPalette": [
                {
                    "command": "extension.helloWorld",
                    "when": "editorLangId == 'python'"
                }
            ]
        },
		"commands": [
			{
				"command": "extension.helloWorld",
                "title": "Export to .ipynb"
            }, 
            {
				"command": "extension.helloWorld",
                "title": "PyToIpynb"
            }, 
            {
                "command": "extension.helloWorld",
				"title": "Save to .ipynb"
            }
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
